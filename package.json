{
  "name": "react-isomorphic-starterkit",
  "description": "Isomorphic starterkit with server-side React rendering.",
  "version": "4.3.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/react-isomorphic-starterkit.git"
  },
  "homepage": "https://github.com/RickWong/react-isomorphic-starterkit",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit"
  ],
  "main": "babel.server.js",
  "scripts": {
    "start": "node --harmony ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "watch": "npm run start"
  },
  "dependencies": {
    "babel": "6.3.26",
    "babel-core": "6.3.26",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "fetch-plus": "3.6.1",
    "fetch-plus-bearerauth": "3.5.0",
    "fetch-plus-json": "3.6.0",
    "history": "1.17.0",
    "isomorphic-fetch": "2.2.0",
    "koa": "1.1.2",
    "koa-proxy": "0.4.1",
    "koa-static": "1.5.2",
    "piping": "0.3.0",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-inline-css": "2.0.1",
    "react-router": "1.0.3",
    "react-transmit": "3.1.5"
  },
  "devDependencies": {
    "babel-loader": "6.2.0",
    "babel-preset-react-hmre": "1.0.0",
    "json-loader": "0.5.4",
    "koa-webpack-dev-middleware": "1.1.0",
    "webpack": "1.12.9",
    "webpack-hot-middleware": "2.6.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
